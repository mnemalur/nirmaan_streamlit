# Databricks Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to Git (it's in .gitignore)

# Databricks Connection
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-databricks-token-here

# Genie Configuration
GENIE_SPACE_ID=your-genie-space-id

# Patient Data Configuration
PATIENT_CATALOG=main
PATIENT_SCHEMA=clinical

# Vector Search Configuration
VECTOR_CATALOG=main
VECTOR_SCHEMA=vector_functions
VECTOR_FUNCTION=diabetes_standard_lookup

# SQL Warehouse Configuration
SQL_WAREHOUSE_ID=your-sql-warehouse-id

